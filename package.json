{
  "name": "style-ui-package",
  "version": "1.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/style-ui-package.umd.cjs",
  "module": "./dist/style-ui-package.js",
  "types": "./dist/style-ui-package.d.ts",
  "exports": {
    ".": {
      "import": "./dist/style-ui-package.js",
      "require": "./dist/style-ui-package.umd.cjs"
    },
    "./dist/style.css": "./dist/style-ui-package.css"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "prebuild": "if exist dist rmdir /s /q dist",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^4.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
